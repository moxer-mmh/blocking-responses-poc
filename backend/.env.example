# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
DEFAULT_MODEL=gpt-4o-mini
JUDGE_MODEL=gpt-4o-mini

# Logging Configuration
LOG_LEVEL=INFO

# Security Settings
DELAY_TOKENS=20
DELAY_MS=250
RISK_THRESHOLD=1.0
JUDGE_THRESHOLD=0.8
ENABLE_JUDGE=true

# CORS Configuration (use * for all origins, or comma-separated list)
CORS_ORIGINS=*